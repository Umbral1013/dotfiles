#
# Bash aliases.
#

alias gh='history | grep'
alias encrypt='gpg -c --no-symkey-cache --cipher-algo AES256'
alias ll='ls -lah'
alias rm='rm -i'
alias mv='mv -vu'
alias vimrc="$EDITOR $HOME/.vim/vimrc"
alias gg='env LD_BIND_NOW=1 gamemoderun'
alias cl='clear'
alias e="$VISUAL"

alias gd='git diff'
alias gl='git log --pretty'
alias gc='git commit -am'
alias gp='git push'
alias gstat='git status'
alias gpull='git pull'
