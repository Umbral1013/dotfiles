#
# Bash aliases.
#

alias gh='history | grep'
alias encrypt='gpg -c --no-symkey-cache --cipher-algo AES256'
alias ll='ls -lah'
alias rm='rm -i'
alias mv='mv -vu'
alias vimrc="$EDITOR $HOME/.vim/vimrc"
alias gg='env LD_BIND_NOW=1 gamemoderun'

alias gd='/usr/bin/git diff'
alias gl='/usr/bin/git log --pretty'
alias gc='/usr/bin/git commit -am'
alias gpush='/usr/bin/git push'
alias gstat='/usr/bin/git status'
alias gpull='/usr/bin/git pull'
